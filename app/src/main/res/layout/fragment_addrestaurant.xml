<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.restaurant.addrestaurant">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/addrestaurant"

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".addsongs">


        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="211dp"
            android:layout_height="117dp"

            android:layout_marginTop="4dp"
            android:layout_marginEnd="108dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/restu" />

        <EditText
            android:id="@+id/restname"
            android:layout_width="380dp"
            android:layout_height="63dp"

            android:layout_marginTop="116dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/edittext2"
            android:ems="10"
            android:hint="    name"
            android:inputType="textPersonName"
            android:paddingStart="4dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView12" />

        <EditText
            android:id="@+id/restreat"
            android:layout_width="380dp"
            android:layout_height="63dp"

            android:layout_marginTop="80dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/edittext2"
            android:ems="10"
            android:hint="   rate"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/restaddres" />

        <EditText
            android:id="@+id/restaddres"
            android:layout_width="380dp"
            android:layout_height="63dp"
            android:layout_marginTop="80dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/edittext2"
            android:ems="10"
            android:hint="   Address"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/restname" />

        <EditText
            android:id="@+id/reatlocation"
            android:layout_width="380dp"
            android:layout_height="63dp"
            android:layout_marginTop="76dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/edittext2"
            android:ems="10"
            android:hint="   location Latitude"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/restreat" />

        <EditText
            android:id="@+id/reatlocation2"
            android:layout_width="380dp"
            android:layout_height="63dp"
            android:layout_marginTop="156dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/edittext2"
            android:ems="10"
            android:hint="   location Longitude"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/restreat" />

        <Button
            android:id="@+id/addrest"
            android:layout_width="379dp"
            android:layout_height="70dp"
            android:layout_marginTop="160dp"
            android:layout_marginEnd="16dp"
            android:backgroundTint="#DBCFFB"
            android:text="add"

            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/reatlocation" />

        <Button
            android:id="@+id/button"
            android:layout_width="147dp"
            android:layout_height="177dp"
            android:layout_marginStart="32dp"
            android:layout_marginBottom="42dp"
            android:background="@drawable/cardbutt"
            android:backgroundTint="#DBCFFB"
            android:rotation="117"
            android:rotationX="13"
            android:rotationY="25"
            android:text=""
            app:layout_constraintBottom_toTopOf="@+id/restname"
            app:layout_constraintStart_toEndOf="@+id/imageView12" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>